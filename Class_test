import json

class Card:
    kinds =pineapple', 'coconut', 'fig', 'orange', 'rambutan', 'pomegranate',
             'lime', 'ocado', 'carambola', 'acai berry', 'papaya',
             'lychee','banana']

    card_counts {'pineapple': 3,
                   'coconut': 6,
 ...
                   }

    def __init__(self, kind):
        if kind not in.kinds:
            raise ValueError(f"Invalid card kind: {kind}")
        self = kind

    def __str__(self):
        return f"Card(kind={self.kind})"

    def score(self, multiplier):
        if self.kind not in self.kinds:
            raise ValueError(f"Invalid card: {self.kind}")
        return self.card_counts[self.kind] * multiplier

    save(self):
        return json.dumps({'kind': self.kind})

    @staticmethod
    def load(json_str):
        data = json.loads(json_str)
        return Card['kind'])